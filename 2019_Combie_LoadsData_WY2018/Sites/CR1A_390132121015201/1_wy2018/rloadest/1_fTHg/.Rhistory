fTHg_CR1A<-importRDB("fTHg_CR1A_R.txt")
CR1AQ<-importRDB("CR1A_QR.txt")
fTHg_CR1Am1 <- loadReg(fTHg ~model(1), data = fTHg_CR1A, flow="Flow", dates = "Dates" ,conc.units="ng/L" , station = "Combie-In")
fTHg_CR1Am1
fTHg_CR1Am2 <- loadReg(fTHg ~model(2), data = fTHg_CR1A, flow="Flow", dates = "Dates" ,conc.units="ng/L" , station = "Combie-In")
fTHg_CR1Am2
fTHg_CR1Am3 <- loadReg(fTHg ~model(3), data = fTHg_CR1A, flow="Flow", dates = "Dates" ,conc.units="ng/L" , station = "Combie-In")
fTHg_CR1Am3
fTHg_CR1Am4 <- loadReg(fTHg ~model(4), data = fTHg_CR1A, flow="Flow", dates = "Dates" ,conc.units="ng/L" , station = "Combie-In")
fTHg_CR1Am4
fTHg_CR1Am5 <- loadReg(fTHg ~model(5), data = fTHg_CR1A, flow="Flow", dates = "Dates" ,conc.units="ng/L" , station = "Combie-In")
fTHg_CR1Am5
fTHg_CR1Am6 <- loadReg(fTHg ~model(6), data = fTHg_CR1A, flow="Flow", dates = "Dates" ,conc.units="ng/L" , station = "Combie-In")
fTHg_CR1Am6
fTHg_CR1Am7 <- loadReg(fTHg ~model(7), data = fTHg_CR1A, flow="Flow", dates = "Dates" ,conc.units="ng/L" , station = "Combie-In")
fTHg_CR1Am7
fTHg_CR1Am8 <- loadReg(fTHg ~model(8), data = fTHg_CR1A, flow="Flow", dates = "Dates" ,conc.units="ng/L" , station = "Combie-In")
fTHg_CR1Am8
fTHg_CR1Am9 <- loadReg(fTHg ~model(9), data = fTHg_CR1A, flow="Flow", dates = "Dates" ,conc.units="ng/L" , station = "Combie-In")
fTHg_CR1Am9
plot(fTHg_CR1Am2,ann=FALSE)
title(main = "390132121015201_m2_fTHg Response vs Fitted Values",xlab = "Fitted Values",ylab = "Response Values")
plot(fTHg_CR1Am2,which = 2,set.up = F)
title(main = "390132121015201_m2_fTHg Residuals vs Fitted Values")
plot(fTHg_CR1Am2,which = 3,set.up = F)
title(main = "390132121015201_m2_fTHg Assessing Heteroscedasticity")
plot(fTHg_CR1Am2,which = 4,set.up = F)
title(main = "390132121015201_m2_fTHg Correlogram of Samples")
plot(fTHg_CR1Am2,which = 5,set.up = F)
title(main="390132121015201_m2_fTHg Normal Discharge")
plot(fTHg_CR1Am2,which = 6,set.up = F)
title(main="390132121015201_m2_fTHg Box Plot of Loads")
fTHg_CR1A_load<-predLoad(fTHg_CR1Am2,CR1AQ,load.units="kg",by="water year",allow.incomplete = TRUE,conf.int = 0.95,print = TRUE)
write.csv(fTHg_CR1A_load,"1_CR1A_m2_fTHg_Flux_Annual.csv")
fTHg_CR1A_load_day<-predLoad(fTHg_CR1Am2, CR1AQ,load.units = "kg",by="day",allow.incomplete = TRUE,conf.int = 0.90,print = TRUE)
write.csv(fTHg_CR1A_load_day,"1_CR1A_m2_fTHg_Flux_Daily.csv")
