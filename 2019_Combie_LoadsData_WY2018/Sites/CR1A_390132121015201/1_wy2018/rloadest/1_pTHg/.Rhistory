pTHg_CR1A<-importRDB("pTHg_CR1A_R.txt")
CR1AQ<-importRDB("CR1A_QR.txt")
pTHg_CR1Am1 <- loadReg(pTHg ~model(1), data = pTHg_CR1A, flow="Flow", dates = "Dates" ,conc.units="ng/L" , station = "Combie-In")
pTHg_CR1Am1
pTHg_CR1Am2 <- loadReg(pTHg ~model(2), data = pTHg_CR1A, flow="Flow", dates = "Dates" ,conc.units="ng/L" , station = "Combie-In")
pTHg_CR1Am2
pTHg_CR1Am3 <- loadReg(pTHg ~model(3), data = pTHg_CR1A, flow="Flow", dates = "Dates" ,conc.units="ng/L" , station = "Combie-In")
pTHg_CR1Am3
pTHg_CR1Am4 <- loadReg(pTHg ~model(4), data = pTHg_CR1A, flow="Flow", dates = "Dates" ,conc.units="ng/L" , station = "Combie-In")
pTHg_CR1Am4
pTHg_CR1Am5 <- loadReg(pTHg ~model(5), data = pTHg_CR1A, flow="Flow", dates = "Dates" ,conc.units="ng/L" , station = "Combie-In")
pTHg_CR1Am5
pTHg_CR1Am6 <- loadReg(pTHg ~model(6), data = pTHg_CR1A, flow="Flow", dates = "Dates" ,conc.units="ng/L" , station = "Combie-In")
pTHg_CR1Am6
pTHg_CR1Am7 <- loadReg(pTHg ~model(7), data = pTHg_CR1A, flow="Flow", dates = "Dates" ,conc.units="ng/L" , station = "Combie-In")
pTHg_CR1Am7
pTHg_CR1Am8 <- loadReg(pTHg ~model(8), data = pTHg_CR1A, flow="Flow", dates = "Dates" ,conc.units="ng/L" , station = "Combie-In")
pTHg_CR1Am8
pTHg_CR1Am9 <- loadReg(pTHg ~model(9), data = pTHg_CR1A, flow="Flow", dates = "Dates" ,conc.units="ng/L" , station = "Combie-In")
pTHg_CR1Am9
plot(pTHg_CR1Am1,ann=FALSE)
title(main = "390132121015201_m1_pTHg Response vs Fitted Values",xlab = "Fitted Values",ylab = "Response Values")
plot(pTHg_CR1Am1,which = 2,set.up = F)
title(main = "390132121015201_m1_pTHg Residuals vs Fitted Values")
plot(pTHg_CR1Am1,which = 3,set.up = F)
title(main = "390132121015201_m1_pTHg Assessing Heteroscedasticity")
plot(pTHg_CR1Am1,which = 4,set.up = F)
title(main = "390132121015201_m1_pTHg Correlogram of Samples")
plot(pTHg_CR1Am1,which = 5,set.up = F)
title(main="390132121015201_m1_pTHg Normal Discharge")
plot(pTHg_CR1Am1,which = 6,set.up = F)
title(main="390132121015201_m1_pTHg Box Plot of Loads")
pTHg_CR1A_load<-predLoad(pTHg_CR1Am1,CR1AQ,load.units="kg",by="water year",allow.incomplete = TRUE,conf.int = 0.95,print = TRUE)
write.csv(pTHg_CR1A_load,"1_CR1A_m1_pTHg_Flux_Annual.csv")
pTHg_CR1A_load_day<-predLoad(pTHg_CR1Am1, CR1AQ,load.units = "kg",by="day",allow.incomplete = TRUE,conf.int = 0.90,print = TRUE)
write.csv(pTHg_CR1A_load_day,"1_CR1A_m1_pTHg_Flux_Daily.csv")
View(pTHg_CR1A_load)
View(pTHg_CR1A_load_day)
