plot(wwTHg_CR1Am1,ann=FALSE)
title(main = "390132121015201_m1_wwTHg Response vs Fitted Values",xlab = "Fitted Values",ylab = "Response Values")
plot(wwTHg_CR1Am1,which = 2,set.up = F)
title(main = "390132121015201_m1_wwTHg Residuals vs Fitted Values")
plot(wwTHg_CR1Am1,which = 3,set.up = F)
