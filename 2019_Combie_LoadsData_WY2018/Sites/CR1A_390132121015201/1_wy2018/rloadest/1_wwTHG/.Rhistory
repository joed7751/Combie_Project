wwTHg_CR1A_load<-predLoad(wwTHg_CR1Am1,CR1AQ,load.units="kg",by="water year",allow.incomplete = TRUE,conf.int = 0.95,print = TRUE)
write.csv(wwTHg_CR1A_load,"1_CR1A_m1_wwTHg_Flux_Annual.csv")
wwTHg_CR1A_load_day<-predLoad(wwTHg_CR1Am1, CR1AQ,load.units = "kg",by="day",allow.incomplete = TRUE,conf.int = 0.90,print = TRUE)
write.csv(wwTHg_CR1A_load_day,"1_CR1A_m1_wwTHg_Flux_Daily.csv")
